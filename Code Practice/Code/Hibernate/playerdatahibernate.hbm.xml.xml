<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="Player" table="Players">  
          <id name="playerID" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="playerFN" column="playerID" type="string"/>
      <property name="playerLN" column="playerLN" type="string"/>
      <property name="playerUN" column="playerUN" type="string"/>
      <property name="playerEmail" column="playerEmail" type="string"/>
      <property name="playerPhone" column="playerPhone" type="string"/>        
  </class>  
            
 </hibernate-mapping>  